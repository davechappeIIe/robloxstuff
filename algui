local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/miroeramaa/TurtleLib/main/TurtleUiLib.lua"))()

local main = library:Window("Main")
local util = library:Window("Utility")
loadstring(game:HttpGet("https://raw.githubusercontent.com/davechappeIIe/robloxstuff/main/skycastle"))()

main:Button("to map", function()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(19, -43, -46))
end)

main:Button("to custom", function()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-22762, 320, 86569))
end)

main:Button("reset", function()
    game.Players.LocalPlayer.Character.Torso:Remove()
end)

main:Button("godmode", function()
    fireclickdetector(game:GetService("Workspace").abandonedmachine.ClickDetector)
end)

util:Button("Autopickup Items", function()
    for i,v in pairs(workspace:GetChildren()) do
        if v.ClassName == "Tool" and v.Name ~= "TestBlock" then
            v.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        end
    end
end)


util:Label("Credits to Intrer#0421 for UI Lib", Color3.fromRGB(127, 143, 166))
util:Label("Credits to pa#3697 for scripts", Color3.fromRGB(127, 143, 166))
library:Keybind("G")
